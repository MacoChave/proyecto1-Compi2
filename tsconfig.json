        "target": "es6",
        "module": "commonjs",
        "outDir": "./public/javascripts",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["ts/*"],
    "exclude": ["node_modules"]
}